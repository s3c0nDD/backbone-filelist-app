{
  "name": "backbone-filelist-app",
  "version": "0.0.1",
  "author": "s3c0nDD",
  "description": "a simple app to learn JS programming",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s3c0nDD/backbone-filelist-app.git"
  },
  "scripts": {
    "start": "beefy index.js:bundle.js --live",
    "bundle": "browserify index.js -o bundle.js"
  },
  "browserify": {
    "transform": [
      "browserify-swap"
    ]
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "all": {
      "underscore.js$": "lodash"
    }
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "jquery": "^2.2.4",
    "lodash": "^4.13.1",
    "mustache": "^2.2.1"
  },
  "devDependencies": {
    "beefy": "^2.1.6",
    "browserify": "^13.0.1",
    "browserify-swap": "^0.2.2"
  }
}
